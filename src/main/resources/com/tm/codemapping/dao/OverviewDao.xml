<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tm.codemapping.dao.OverviewDao" >
  
  <insert id="insert" parameterType="com.tm.codemapping.pojo.AssetItemPojo" 
  			useGeneratedKeys="true" keyProperty="id">
    insert into assetitem (id, name )
    		values (#{id}, #{name} )
  </insert>
  
  <update id="update" parameterType="com.tutorial.demo.pojo.AssetItemPojo">
    update assetitem set name=#{name}, id=#{id} where id=#{id}
  </update>
  
</mapper>